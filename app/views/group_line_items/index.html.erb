<p id="notice"><%= notice %></p>

<h1>Group Line Items</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Group cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_line_items.each do |group_line_item| %>
      <tr>
        <td><%= group_line_item.product %></td>
        <td><%= group_line_item.group_cart %></td>
        <td><%= link_to 'Show', group_line_item %></td>
        <td><%= link_to 'Edit', edit_group_line_item_path(group_line_item) %></td>
        <td><%= link_to 'Destroy', group_line_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group Line Item', new_group_line_item_path %>
